{
  "name": "y",
  "version": "0.0.1",
  "description": "Example implementation of react-redux",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.2"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --config ./webpack.config.js",
    "test": "NODE_ENV=test jest",
    "lint": "standard --verbose | snazzy",
    "coverage": "istanbul check-coverage",
    "coveralls": "cat ./reports/coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joedunu/react-redux-example.git"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "Anuradha Dunuhappawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joedunu/react-redux-example/issues"
  },
  "homepage": "https://github.com/joedunu/react-redux-example#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "classnames": "^2.2.5",
    "express": "^4.15.2",
    "history": "^4.7.2",
    "jss": "^9.5.0",
    "jss-nested": "^6.0.1",
    "lodash": "^4.17.21",
    "material-ui": "^1.0.0-beta.22",
    "material-ui-icons": "^1.0.0-beta.17",
    "prop-types": "^15.6.0",
    "react": "^16.13.1",
    "react-dom": "^16.2.0",
    "react-modal": "^3.1.10",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-saga": "^0.16.0",
    "styled-components": "^2.4.0"
  },
  "devDependencies": {
    "ajv": "^5.5.2",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.4",
    "babel-loader": "^6.4.1",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "caniuse-db": "^1.0.30000784",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.3",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-jsx": "^4.0.1",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-bin": "^0.62.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^22.0.4",
    "jest-css-modules": "^1.1.0",
    "jest-enzyme": "^4.0.2",
    "jest-junit": "^3.4.1",
    "jest-styled-components": "^4.9.0",
    "json-loader": "^0.5.4",
    "jss-vendor-prefixer": "^7.0.0",
    "postcss": "^6.0.14",
    "postcss-cssnext": "^2.10.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-nested": "^3.0.0",
    "raf": "^3.4.0",
    "react-addons-test-utils": "^16.0.0-alpha.3",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.3.0",
    "resolve-url-loader": "^2.2.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "standard-react": "^4.0.0",
    "style-loader": "^0.16.1",
    "webpack": "3.10.0",
    "webpack-dev-server": "^2.11.0"
  },
  "standard": {
    "ignore": [
      "/node_modules/",
      "retry.js",
      "/app/e2e/publish/"
    ],
    "globals": [
      "it",
      "console",
      "protractor",
      "browser",
      "by",
      "element",
      "expect",
      "jest",
      "describe",
      "assert",
      "test"
    ]
  },
  "jest": {
    "globals": {
      "ENV": "DEV"
    },
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "transformIgnorePatterns": [],
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "app/src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "app/src/index.js",
      "app/storybook.js",
      "spec.js",
      ".stories.js",
      "/assets/",
      "/sagas/"
    ],
    "coverageDirectory": "./reports/coverage/",
    "coverageReporters": [
      "json",
      "html",
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 32,
        "branches": 9.5,
        "functions": 10.5,
        "lines": 34
      }
    },
    "testResultsProcessor": "./app/config/jest/resultsProcessor",
    "moduleNameMapper": {
      "\\.(css)$": "jest-css-modules"
    },
    "setupFiles": [],
    "setupTestFrameworkScriptFile": "<rootDir>/app/config/jest/testFrameworkSetup",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "babel": {
    "ignore": "node_modules",
    "presets": [
      "react",
      "flow",
      [
        "es2015",
        {
          "modules": false,
          "loose": true
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  }
}
